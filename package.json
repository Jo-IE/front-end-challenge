{
  "name": "ospin-front-end-code-challenge",
  "version": "1.0.0",
  "engineStrict": true,
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@jsonforms/core": "^2.3.1",
    "@jsonforms/material-renderers": "^2.3.1",
    "@jsonforms/react": "^2.3.1",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "babel-preset-react-app": "^9.0.0",
    "react": "^16.10.2",
    "react-beautiful-dnd": "^11.0.4",
    "react-dom": "^16.8.6",
    "react-jss": "^10.0.0",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.0.1",
    "semantic-ui-react": "^0.88.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "history": "^4.10.1",
    "is-buffer": "^2.0.4",
    "react-router": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/assetMock.js",
      "\\.(css|less)$": "<rootDir>/test/mocks/assetMock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
